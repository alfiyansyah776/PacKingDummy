<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ListRumahKemasanActivity">

    <TextView
        android:id="@+id/textView9"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:text="Rumah Kemasan Terdekat"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_list_rumah_kemasan_terdekat"
        android:layout_width="354dp"
        android:layout_height="180dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />


    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Rumah Kemasan"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/rv_list_rumah_kemasan_terdekat"
        app:layout_constraintTop_toBottomOf="@+id/rv_list_rumah_kemasan_terdekat" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_list_rumah_kemasan"
        android:layout_width="354dp"
        android:layout_height="380dp"
        android:layout_marginTop="6dp"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />
</androidx.constraintlayout.widget.ConstraintLayout>